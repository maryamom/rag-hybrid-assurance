# Configuration des bases de donn√©es
# Copier ce fichier vers database.yaml et modifier les valeurs

postgres:
  host: localhost
  port: 5432
  database: data
  user: postgres
  password: your_password_here
  pool_size: 10
  max_overflow: 20

neo4j:
  uri: bolt://localhost:7687
  user: neo4j
  password: your_password_here
  max_connection_lifetime: 3600
  max_connection_pool_size: 50

qdrant:
  host: localhost
  port: 6333
  collection_name: insurance_embeddings
  vector_size: 384
  distance: Cosine

redis:
  host: localhost
  port: 6379
  password: null
  db: 0
  max_connections: 10
